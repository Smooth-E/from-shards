    chapterStory: "\u0411\u043E\u0436\u0435, \u044F \u043F\u0438\u0441\u0430\u043B\u0430
      \u044D\u0442\u043E \u0442\u043E\u0433\u0434\u0430: \u201C\u2026\u0434\u043E\u0440\u0440\u0433\u043E\u0439
      \u0434\u043D\u0435\u0432\u043D\u043A! \u0414\u0441\u0442\u0430\u044E \u044F
      \u0442\u0435\u0431\u044F \u0432\u0441\u0451 \u0440\u0435\u0436\u0435, \u0438\u0437\u0432\u0438\u043D\u0442\u0438\u043B\u044F\u044E\u0441\u044C!
      \u0416\u0438\u0437\u043D\u044C \u043C\u043E\u044F \u043B\u0435\u0442\u0438\u0442
      \u0432 \u0410\u0414, \u044F \u043B\u0438\u0448\u044C \u0432\u0438\u0431\u0438\u0440\u0430\u044E
      \u043F\u043E\u0434\u0445\u043E\u0434\u044F\u0449\u0443\u044E \u043C\u0443\u0437\u043A\u0443.
      \u041C\u0430\u043C\u0430 \u043D\u0435 \u0441\u043F\u0440\u0430\u0432\u0438\u043B\u0430\u0441\u044C.
      \u041E\u0442\u0435\u0446 \u0449\u0451 \u0434\u044B\u0448\u0438\u0442. \u0412\u0440\u0430\u0447\u0438\u2026
      \u0414\u0447\u0442\u043E \u0442\u0430\u043C, \u043E\u043D\u0438 \u0436\u0435
      \u0433\u043E\u0432\u043E\u0440\u0438\u043B\u0438 \xAB\u0432\u043E\u0442 \u0442\u0430\u0431\u043B\u0442\u043A\u0438,
      \u043F\u0435\u0439\u0442\u0435\u0438 \u0443\u0441\u043F\u043E\u043A\u043E\u0438\u0442\u0435\u0441\u044C\u2026\xBB.
      \u0410 \u043A\u0442\u043E \u0441\u043A\u0430\u0436\u0435\u0442 \u0447\u0442\u043E
      \u0434\u0435\u043B\u0430\u0442\u044C \u043A\u043E\u0433\u0434\u0430 \u043C\u0430\u0442\u044C
      \u043F\u0440\u0445\u043E\u0434\u0438\u0442 \u0432\u043E \u0441\u043D\u0430\u0445?
      \u041A\u0430\u043A \u0443\u043B\u0430\u0431\u0430\u0442\u044C\u0441\u044F \u043B\u044E\u0434\u044F\u043C
      \u043D\u0430 \u0440\u0430\u0431\u043E\u0442\u0435? \u0427\u0442\u043E \u043E\u0442\u0432\u0447\u0430\u0442\u044C
      \u0434\u0440\u0440\u0443\u0437\u044C\u044F\u043C \u043D\u0430 \u0438\u0445
      \u0433\u0440\u0443\u0441\u0442\u043D\u044B\u0435 \u0441\u043E\u0431\u0430\u0447\u044C\u0438
      \u0433\u043B\u0430\u0437\u0430, \u0438 \u0432\u043E\u0442 \u044D\u0442\u043E
      \xAB\u0441\u043E\u0431\u043E\u043B\u0435\u0437\u043D\u0443\u044E\xBB?!? \u0415\u0441\u043B\u0438
      \u0431\u044B \u0437\u0430 \u043A\u0430\u0436\u0434\u043E\u0435 \xAB\u0441\u043E\u0431\u043E\u043B\u0435\u0437\u043D\u0443\u044E\xBB
      \u043C\u043D\u0435 \u0434\u0430\u0432\u0430\u043B\u0438 \u0434\u043E\u043B\u043B\u0430\u0440,
      \u044F \u0431\u044B \u043A\u0443\u043F\u0438\u043B\u0430 \u0431\u0443\u0445\u043B\u0430.
      \u0421\u043E\u0431\u0441\u0442\u0432\u043D\u043D\u043E, \u0442\u0430\u043A
      \u0432 \u0438\u0442\u043E\u0433\u0435 \u044F\u0438\u0441\u0434\u0435\u043B\u0430\u043B\u0430.
      \u0412\u0441\u0451 \u043A \u0445\u0435\u0440\u0430\u043C! \u0418\u043B\u0438
      \u043D\u0435 \u0432\u0441\u0451. \u0413\u043E\u0441\u043F\u043E\u0434\u0438,
      \u0410\u043A\u0432\u0430\u043C\u0435\u043D, \u0417\u0435\u043B\u0438\u0431\u043E\u0431\u0430,
      \u043A\u0442\u043E \u0443\u0433\u043E\u0434\u043D\u043E, \u0435\u0441\u043B\u0438
      \u0442\u044B \u0435\u0441\u0442\u044C \u2013 \u043F\u043E\u043C\u043E\u0433\u0438
      \u043C\u043D\u0435 \u043F\u043E\u0436\u0430\u043B\u0443\u0439\u0441\u0442\u0430.
      \u042F \u043D\u0435 \u0432\u044B\u0432\u043E\u0436\u0443.\u201D"
