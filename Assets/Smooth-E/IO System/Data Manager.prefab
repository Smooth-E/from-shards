    chapterStory: "\u0414\u0430, \u044D\u0442\u043E \u043E\u0431\u043E \u043C\u043D\u0435.
      \u042F \u0432\u0435\u0434\u0443 \u0437\u0430\u043F\u0438\u0441\u0438, \u0441\u043A\u043E\u043B\u044C\u043A\u043E
      \u0441\u0435\u0431\u044F \u043F\u043E\u043C\u043D\u044E. \u0412 \u0442\u0435
      \u0434\u043D\u0438, \u043A\u043E\u0433\u0434\u0430 \u0436\u0438\u0437\u043D\u044C
      \u043A\u0430\u0437\u0430\u043B\u0430\u0441\u044C \u043D\u0435\u0432\u044B\u043D\u043E\u0441\u0438\u043C\u043E\u0439,
      \u044D\u0442\u043E \u0432\u0441\u0435\u0433\u0434\u0430 \u043F\u043E\u043C\u043E\u0433\u0430\u043B\u043E
      \u043C\u043D\u0435 \u0440\u0430\u0437\u043E\u0431\u0440\u0430\u0442\u044C\u0441\u044F
      \u0432 \u0441\u0435\u0431\u0435. \u0418 \u0431\u044B\u043B\u043E \u0434\u0435\u0439\u0441\u0442\u0432\u0438\u0442\u0435\u043B\u044C\u043D\u043E
      \u0443\u0436\u0430\u0441\u043D\u043E, \u044F \u0442\u043E\u0433\u0434\u0430
      \u043F\u043E\u0442\u0435\u0440\u044F\u043B\u0430 \u0440\u043E\u0434\u0438\u0442\u0435\u043B\u0435\u0439
      \u0432 \u043F\u043E\u0436\u0430\u0440\u0435. \u0418 \u044F \u043F\u043E\u0435\u0445\u0430\u043B\u0430
      \u043A \u0441\u0432\u043E\u0435\u043C\u0443 \u043F\u0430\u0440\u043D\u044E,
      \u0447\u0442\u043E\u0431\u044B \u043E\u043D \u043F\u043E\u0434\u0434\u0435\u0440\u0436\u0430\u043B
      \u043C\u0435\u043D\u044F \u0432 \u0442\u043E\u0439 \u0442\u0440\u0443\u0434\u043D\u043E\u0439
      \u0441\u0438\u0442\u0443\u0430\u0446\u0438\u0438, \u043D\u043E \u0437\u0430\u0441\u0442\u0430\u043B\u0430
      \u0435\u0433\u043E \u0441 \u043B\u044E\u0431\u043E\u0432\u043D\u0438\u0446\u0435\u0439.
      \u041A\u0430\u043A \u043F\u043E \u0437\u0430\u043A\u043E\u043D\u0443 \u041C\u0451\u0440\u0444\u0438,
      \u0432\u0441\u0435 \u0431\u0435\u0434\u044B \u043D\u0430\u0447\u0430\u043B\u0438
      \u0432\u0430\u043B\u0438\u0442\u044C\u0441\u044F \u043D\u0430 \u043C\u0435\u043D\u044F
      \u043E\u0434\u043D\u0430 \u0437\u0430 \u0434\u0440\u0443\u0433\u043E\u0439.
      \u0418, \u043A\u0430\u0436\u0435\u0442\u0441\u044F, \u043C\u0435\u043D\u044F
      \u044D\u0442\u043E \u0441\u043B\u043E\u043C\u0430\u043B\u043E. \u041F\u0435\u043F\u0435\u043B
      \u043A\u0440\u0443\u0436\u0438\u0442\u0441\u044F \u0432 \u0432\u043E\u0437\u0434\u0443\u0445\u0435
      \u0441\u043D\u0435\u0436\u0438\u043D\u043A\u0430\u043C\u0438, \u0438 \u044D\u0442\u043E
      \u043A\u0440\u0430\u0441\u0438\u0432\u043E\u2026 "
