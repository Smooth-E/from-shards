    chapterStory: "\u0412\u0441\u0451, \u0447\u0435\u0433\u043E \u044F \u0441\u0435\u0439\u0447\u0430\u0441
      \u0445\u043E\u0447\u0443 \u2013 \u044D\u0442\u043E \u0440\u0430\u0441\u0442\u0432\u043E\u0440\u0438\u0442\u044C\u0441\u044F
      \u0432 \u044D\u0442\u043E\u043C \u0445\u043E\u0440\u043E\u0432\u043E\u0434\u0435
      \u043F\u043B\u0430\u043C\u0435\u043D\u0438, \u0438 \u043D\u0435 \u0432\u0438\u0434\u0435\u0442\u044C\u2026
      \u043D\u0435 \u0432\u0438\u0434\u0435\u0442\u044C \u043E\u0431\u0443\u0433\u043B\u0435\u043D\u043D\u044B\u0445
      \u0441\u0442\u0440\u043E\u043F\u0438\u043B, \u0447\u0442\u043E \u0442\u043E\u0440\u0447\u0430\u0442
      \u0441\u043B\u043E\u0432\u043D\u043E \u043A\u043E\u0441\u0442\u0438\u2026 \u043D\u0435
      \u0432\u0438\u0434\u0435\u0442\u044C \u043C\u0430\u0448\u0438\u043D \u043F\u043E\u0436\u0430\u0440\u043D\u044B\u0445,
      \u0437\u0430\u043B\u0438\u0432\u0430\u044E\u0449\u0438\u0445 \u043E\u0441\u0442\u0430\u0442\u043A\u0438
      \u043D\u0430\u0448\u0435\u0433\u043E \u0434\u043E\u043C\u0430, \u043D\u0435
      \u0432\u0438\u0434\u0435\u0442\u044C \u043A\u0430\u043A \u0441\u043A\u043E\u0440\u0430\u044F
      \u043F\u043E\u043C\u043E\u0449\u044C \u0443\u0432\u043E\u0437\u0438\u0442 \u0432
      \u0431\u043E\u043B\u044C\u043D\u0438\u0446\u0443 \u0440\u043E\u0434\u0438\u0442\u0435\u043B\u0435\u0439,
      \u043D\u0435 \u043E\u0442\u0432\u0435\u0447\u0430\u0442\u044C \u043D\u0430
      \u0442\u044B\u0441\u044F\u0447\u0443 \u0438\u0434\u0438\u043E\u0442\u0441\u043A\u0438\u0445
