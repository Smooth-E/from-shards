    chapterStory: "\u0416\u0438\u0442\u044C. \u0414\u0440\u043E\u0436\u0430\u0449\u0438\u043C\u0438
      \u0440\u0443\u043A\u0430\u043C\u0438 \u044F \u0432\u044B\u0432\u043E\u0436\u0443
      \u044D\u0442\u043E \u0441\u043B\u043E\u0432\u043E. \u0418 \u0433\u043E\u0442\u043E\u0432\u0430
      \u043F\u043E\u0432\u0442\u043E\u0440\u0438\u0442\u044C \u0435\u0433\u043E \u0435\u0449\u0451
      \u043C\u0438\u043B\u043B\u0438\u043E\u043D \u0440\u0430\u0437. \u0416\u0438\u0442\u044C\u0436\u0438\u0442\u044C\u0436\u0438\u0442\u044C\u0436\u0438\u0442\u044C.
      \u041C\u043D\u0435 \u043F\u0440\u0438\u0448\u043B\u043E\u0441\u044C \u043E\u043A\u0443\u043D\u0443\u0442\u044C\u0441\u044F
      \u0432 \u0432\u043E\u0441\u043F\u043E\u043C\u0438\u043D\u0430\u043D\u0438\u044F
      \u0438 \u043F\u0440\u043E\u0439\u0442\u0438 \u0447\u0435\u0440\u0435\u0437
      \u0432\u0441\u0451 \u0441\u043D\u043E\u0432\u0430, \u0447\u0442\u043E\u0431\u044B
      \u0432\u0441\u0451 \u043F\u0435\u0440\u0435\u043E\u0441\u043C\u044B\u0441\u043B\u0438\u0442\u044C.
      \u041F\u043E\u0447\u0435\u043C\u0443 \u0447\u0435\u043B\u043E\u0432\u0435\u043A\u0443
      \u043D\u0443\u0436\u043D\u043E \u0432\u0441\u0451 \u043F\u043E\u0442\u0435\u0440\u044F\u0442\u044C,
      \u0447\u0442\u043E\u0431\u044B \u043D\u0430\u0447\u0430\u0442\u044C \u0446\u0435\u043D\u0438\u0442\u044C
      \u0436\u0438\u0437\u043D\u044C? \u0425\u043E\u0442\u044C \u043C\u043E\u0438
      \u0440\u0443\u043A\u0438 \u0434\u0440\u043E\u0436\u0430\u0442, \u0438 \u0441\u0442\u0440\u0430\u043D\u0438\u0446\u0430
      \u0440\u0430\u0437\u0431\u0443\u0445\u043B\u0430 \u043E\u0442 \u0441\u043B\u0451\u0437,
      \u044F \u0433\u043E\u0442\u043E\u0432\u0430 \u0441\u043E\u0447\u0438\u043D\u0438\u0442\u044C
      \u0435\u0449\u0451 \u0442\u044B\u0441\u044F\u0447\u0443 \u043F\u043E\u0432\u043E\u0434\u043E\u0432
      \u0436\u0438\u0442\u044C. \u041F\u0443\u0441\u0442\u044C \u044F \u0443\u0436\u0435
      \u043D\u0435 \u0442\u0430, \u0447\u0442\u043E \u0440\u0430\u043D\u044C\u0448\u0435,
      \u0442\u0435\u043F\u0435\u0440\u044C \u044F \u0431\u0443\u0434\u0443 \u0436\u0438\u0442\u044C
      \u0438\u0437\u043E \u0432\u0441\u0435\u0445 \u0441\u0438\u043B. \u0417\u0430
      \u0441\u0435\u0431\u044F, \u0437\u0430 \u0440\u043E\u0434\u0438\u0442\u0435\u043B\u0435\u0439,
      \u0437\u0430 \u0442\u0435\u0445, \u043A\u0442\u043E \u043D\u0435 \u0443\u0441\u043F\u0435\u043B
